<div class="jumbotron">
  <h1 class="display-4"><%= @list.title %></h1>
  <p class="lead"><%= render @list %></p>
  <hr class="my-4">
  <%= link_to "Edit", edit_list_path(@list), class:"btn btn-secondary" %> |
  <%= link_to "Back", lists_path, class:"btn btn-dark" %>
</div>
<div>

<%= button_to 'Delete', @list, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-danger" %>

</div>

